    "def engcheck(string): \n",
    "    strike = 0\n",
    "    for char in string: \n",
    "        if ord(char) > 127:\n",
    "            strike +=1\n",
    "    if strike > 3: \n",
    "        return False\n",
    "    return True\n",
    "print(engcheck('Instagram'))\n",
    "print(engcheck('Áà±Â•áËâ∫PPS -„ÄäÊ¨¢‰πêÈ¢Ç2„ÄãÁîµËßÜÂâßÁÉ≠Êí≠'))\n",
    "print(engcheck('Docs To Go‚Ñ¢ Free Office Suite'))\n",
    "print(engcheck('Instachat üòú'))"
   ]
